        "def meat_encode(l: list):\n",
        "  if sum(l) == 0:\n",
        "    return np.insert(l, 0, 1)\n",
        "  else:\n",
        "    return np.insert(l, 0, 0)"
      ],
      "metadata": {
        "id": "SMCq0FHd91Sx"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "sampled['ohe_meat'] = sampled.apply(lambda x: meat_encode([x.Beef, x.Pork, x.Chicken, x.Duck, x.Seafood]), axis=1)\n",
        "sampled['codelist_meat'] = sampled[\"ohe_meat\"].apply(lambda x: np.where(x == 1)[0])\n",
        "sampled = sampled.explode('codelist_meat')"
      ],
